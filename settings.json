{
    "workbench.colorTheme": "Î²ui - Eighties Dark",
    "workbench.iconTheme": "vscode-icons",
    "window.zoomLevel": 0,
    "editor.fontSize": 13,
    "editor.fontFamily": "Monaco",
    "editor.wordWrap": "on",
    "editor.showFoldingControls": "always",
    "editor.tabSize": 4,
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#f991577f",
        "editor.inactiveSelectionBackground": "#f991574f",

        "editor.selectionHighlightBorder": "#f991578f",
        "editor.selectionHighlightBackground": "#ffffff00",

        "editorBracketMatch.border": "#f991578f",
        
        // "editor.foreground": "#ff0000",
        "editor.background": "#282828",
        "editorGutter.background": "#282828",

        "list.hoverForeground": "#f99157",
        "list.hoverBackground": "#00000000",
        "list.inactiveSelectionForeground": "#f99157",
    },

    "editor.tokenColorCustomizations": {
        "strings": "#FFE792",
        "textMateRules": [{
                "scope": [
                    "variable",
                    "variable.other",
                    "variable.name",
                    "variable.other.constant"
                ],
                "settings": {
                    "foreground": "#eeeeee",
                    "fontStyle": ""
                }
            },{
                "scope": "punctuation",
                "settings": {
                    "foreground": "#888888"
                }
            }, {
                "scope": "meta.function-call entity.name.function",
                "settings": {
                    "foreground": "#71A1D0"
                }
            }, {
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#99cc99"
                }
            }, {
                "scope": "variable.parameter",
                "settings": {
                    "foreground": "#f99157"
                }
            }, {
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#ffcc66"
                }
            }, {
                "scope": "entity.name",
                "settings": {
                    "foreground": "#71A1D0"
                }
            }, {
                "name": "Object Literal",
                "scope": [
                    "meta.object-literal.key"
                ],
                "settings": {
                    "foreground": "#FFE792"
                }
            }
        ]
    },
    "files.associations": {
        "*.js": "typescriptreact",
        "*.jsx": "javascriptreact"
    },
    "vsicons.dontShowNewVersionMessage": true,



    // Specify the refresh interval in seconds
    "crypto-ticker.interval": 60,
    // Specify the opening period
    "crypto-ticker.period": "24HOUR",
    // Customize your high and low colors to suite your status bar
    "crypto-ticker.higherColor": "white",
    "crypto-ticker.lowerColor": "white",
    // Customize your tickers
    "crypto-ticker.tickers": [
        {
            // The base cryptocurrency symbol
            "symbol": "BTC",
            // The comparison currency symbol
            "currency": "USD",
            // The cryptocurrency exchange
            // "exchange": "Coinbase",
            // Create a custom ticker template
            "template": "{symbol} {price}"
        },
        {
            // The base cryptocurrency symbol
            "symbol": "EOS",
            // The comparison currency symbol
            "currency": "USD",
            // The cryptocurrency exchange
            // "exchange": "Coinbase",
            // Create a custom ticker template
            "template": "{symbol} {price}"
        },
    ],
    
}